class Account:
    def __init__(self,bal,acc):
        self.balance=bal
        self.account=acc
    def debit(self,amount):
        self.balance==amount
        print('rs',amount,'is debited')
        print('balance amount is',self.get_balance())
    def credit(self,amount):
        self.balance+=amount
        print("rs",amount,"is credited")
        print('balance amount is',self.get_balance())
    def get_balance(self):
        return self.balance
acc1=Account(100000,1245)
acc1.debit(1000)
acc1.credit(500)
