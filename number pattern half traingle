//right
n=int(input('enter no'))
sum=0
for i in range(n,-1,-1):
    for j in range(n-i):
        sum=sum+1
        print(sum,end=' ')
    print()

//inverted
n=int(input('enter no'))
sum=0
for i in range(n):
    for j in range(n):
        sum=sum+1
        print(sum,end=' ')
    print()
//square or rectangke
